<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <connectionStrings>
    <add name="CarRental" connectionString="data source=(LocalDb)\MSSQLLocalDB;initial catalog=CarRental.Data.CarRentalContext;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="CarRental.Business.Services.InventoryService">
        <endpoint address="net.tcp://localhost:8080/InventoryService"
                  binding="netTcpBinding"
                  contract="CarRental.Business.Contracts.IInventoryService" />
      </service>
    </services>
    <bindings>
      <netTcpBinding>
        <binding transactionFlow="true">
          <reliableSession enabled="true"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>